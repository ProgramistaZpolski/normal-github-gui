{
    "name": "Normal GitHub",
    "version": "1.4",
    "description": "Revert GitHub to its state before the UI redesign.",
    "manifest_version": 2,
    "content_scripts": [
        {
          "matches": ["https://github.com/*", "https://*.github.com/*"],
          "js": ["github-run.js", "css-apply.js"],
          "run_at": "document_end"
        }
    ],
    "icons": {
      "64":"icon64.png",
      "128":"icon128.png"
    }
  }